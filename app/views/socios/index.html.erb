<p id="notice"><%= notice %></p>

<h1>Lista de Sócios</h1>

<table>
  <thead>
    <tr>
      <th>Nome &nbsp;</th>
      <th>Cpf  &nbsp;</th>
      <th>Email  &nbsp;</th>
      <th>Telefone &nbsp;</th>
      <th>Endereco &nbsp;</th>
      <th>Data de Nasc. &nbsp;</th>
      <th>Sexo &nbsp;</th>
      <th>Cadeira &nbsp;</th>
      <th>Senha &nbsp;</th>
      <th>Permissão &nbsp;</th>
      <th>Criado em &nbsp;</th>
      <th>Modificado em &nbsp;</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @socios.each do |socio| %>
      <tr>
        <td><%= socio.nome %></td>
        <td><%= socio.cpf %></td>
        <td><%= socio.email %></td>
        <td><%= socio.telefone %></td>
        <td><%= socio.endereco %></td>
        <td><%= socio.dataNascimento %></td>
        <td><%= socio.sexo %></td>
        <td><%= socio.cadeira %></td>
        <td><%= socio.senha %></td>
        <td><%= socio.permissao %></td>
        <td><%= socio.created_at %></td>
        <td><%= socio.updated_at %></td>
        <td><%= link_to 'Detalhe', socio %></td>
        <td><%= link_to 'Editar', edit_socio_path(socio) %></td>
        <td><%= link_to 'Deletar', socio, method: :delete, data: { confirm: 'Você tem certeza?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Cadastrar Novo Sócio', new_socio_path %>

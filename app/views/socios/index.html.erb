<h1>Lista de Sócios</h1>

<table>
  <thead>
    <tr>
      <th>Nome:</th>
      <th>Cpf:</th>
      <th>Email:</th>
      <th>Telefone:</th>
      <th>Endereco:</th>
      <th>Nascimento:</th>
      <th>Sexo:</th>
      <th>Cadeira:</th>
      <th>Senha:</th>
      <th>Permissão:</th>
      <th>Criado_em:</th>
      <th>Modificado_em:</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @socios.each do |socio| %>
      <tr>
        <td><%= socio.nome %></td>
        <td><%= socio.cpf %></td>
        <td><%= socio.email %></td>
        <td><%= socio.telefone %></td>
        <td><%= socio.endereco %></td>
        <td><%= socio.dataNascimento %></td>
        <td><%= socio.sexo %></td>
        <td><%= socio.cadeira %></td>
        <td><%= socio.senha %></td>
        <td><%= socio.permissao %></td>
        <td><div><%= socio.created_at %></div></td>
        <td><%= socio.updated_at %></td>
        <td><%= link_to 'Detalhe', socio %></td>
        <td><%= link_to 'Editar', edit_socio_path(socio) %></td>
        <td><%= link_to 'Deletar', socio, method: :delete, data: { confirm: 'Você tem certeza?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>


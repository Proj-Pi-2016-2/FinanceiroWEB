<p id="notice"><%= notice %></p>

<h1>Lista Socios</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Cpf</th>
      <th>Email</th>
      <th>Telefone</th>
      <th>Endereco</th>
      <th>Datanascimento</th>
      <th>Sexo</th>
      <th>Cadeira</th>
      <th>Senha</th>
      <th>Permissao</th>
      <th>Created at</th>
      <th>Updated at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @socios.each do |socio| %>
      <tr>
        <td><%= socio.nome %></td>
        <td><%= socio.cpf %></td>
        <td><%= socio.email %></td>
        <td><%= socio.telefone %></td>
        <td><%= socio.endereco %></td>
        <td><%= socio.dataNascimento %></td>
        <td><%= socio.sexo %></td>
        <td><%= socio.cadeira %></td>
        <td><%= socio.senha %></td>
        <td><%= socio.permissao %></td>
        <td><%= socio.created_at %></td>
        <td><%= socio.updated_at %></td>
        <td><%= link_to 'Show', socio %></td>
        <td><%= link_to 'Edit', edit_socio_path(socio) %></td>
        <td><%= link_to 'Destroy', socio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Socio', new_socio_path %>

<div class="page-header">
	<h1>Log In</h1>
</div>

<%= form_tag sessions_path do %>
	 <div class="form-group">
	 	<%= label_tag :email %>
	 	<%= email_field_tag :eemail, params[:eemail],
	 	class: "form-control" %>
	 </div>
	 <div class="form-group">
	 	<%= label_tag :senha %>
	 	<%= password_field_tag :senha, nil,
	 	class: "form-control" %>
	 </div>
	 <%= submit_tag "Log In", class: "btn btn-primary" %>
<% end %>
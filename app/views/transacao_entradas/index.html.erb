<p id="notice"><%= notice %></p>

<ul class="nav nav-pills">
  <li class="active">
    <a href="/transacao_entradas/new">Transação Entrada</a>
  </li>
  <li><a href="/transacao_saidas/new">Transação Saida</a></li>
</ul>

<h1>Transação Entradas</h1>

<table>
  <thead>
    <tr>
      <th>Id entrada</th>
      <th>Valor</th>
      <th>Data entrada</th>
      <th>Justifica entrada</th>
      <th>Tipo</th>
      <th>Created at</th>
      <th>Updated at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @transacao_entradas.each do |transacao_entrada| %>
      <tr>
        <td><%= transacao_entrada.id_entrada %></td>
        <td><%= transacao_entrada.valor %></td>
        <td><%= transacao_entrada.data_entrada %></td>
        <td><%= transacao_entrada.justifica_entrada %></td>
        <td><%= transacao_entrada.tipo %></td>
        <td><%= transacao_entrada.created_at %></td>
        <td><%= transacao_entrada.updated_at %></td>
        <td><%= link_to 'Show', transacao_entrada %></td>
        <td><%= link_to 'Edit', edit_transacao_entrada_path(transacao_entrada) %></td>
        <td><%= link_to 'Destroy', transacao_entrada, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Transacao entrada', new_transacao_entrada_path %>

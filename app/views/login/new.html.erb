<h1>Login</h1>
 <%= form_for :login, url: login_index_path do |f| %>
    <div class="field">
      <%= f.label :email %><br />
      <%= f.email_field :email, autofocus: true %>
    </div>
   
    <div class="field">
      <%= f.label :senha %><br />
      <%= f.password_field :senha, autocomplete: "off" %>
    </div>
    <div class="actions">
      <%= f.submit "Entrar" %>
    </div>
  <% end %>
<p id="notice"><%= notice %></p>

<h1>Listing Transacao Saidas</h1>

<table>
  <thead>
    <tr>
      <th>Id saida</th>
      <th>Valor</th>
      <th>Data saida</th>
      <th>Justifica saida</th>
      <th>Img comprov</th>
      <th>Created at</th>
      <th>Updated at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @transacao_saidas.each do |transacao_saida| %>
      <tr>
        <td><%= transacao_saida.id_saida %></td>
        <td><%= transacao_saida.valor %></td>
        <td><%= transacao_saida.data_saida %></td>
        <td><%= transacao_saida.justifica_saida %></td>
        <td><%= transacao_saida.img_comprov %></td>
        <td><%= transacao_saida.created_at %></td>
        <td><%= transacao_saida.updated_at %></td>
        <td><%= link_to 'Show', transacao_saida %></td>
        <td><%= link_to 'Edit', edit_transacao_saida_path(transacao_saida) %></td>
        <td><%= link_to 'Destroy', transacao_saida, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Transacao saida', new_transacao_saida_path %>
